{
  "name": "athvio-for-patients",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web:local": "[ -f .env.local.bak ] && mv .env.local.bak .env.local || true && NODE_ENV=local expo start --web",
    "web:dev": "[ -f .env.local ] && mv .env.local .env.local.bak || true && NODE_ENV=development expo start --web",
    "web:dev2": "[ -f .env.local ] && mv .env.local .env.local.bak || true && NODE_ENV=production expo start --web",
    "web:prod": " NODE_ENV=production expo start --web",
    "lint": "npx prettier --write .",
    "format": "npx prettier --write .",
    "build:dev":"eas build --platform android --profile preview"
  },
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "@expo/metro-runtime": "~3.2.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native/gradle-plugin": "^0.75.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^1.9.3",
    "expo": "^52.0.0",
    "expo-dev-client": "~5.0.9",
    "expo-router": "^4.0.17",
    "expo-splash-screen": "^0.29.21",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.26.13",
    "formik": "^2.4.6",
    "graphql": "^15.8.0",
    "graphql-ws": "^5.16.0",
    "npm-force-resolutions": "^0.0.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-vector-icons": "^10.1.0",
    "react-redux": "^9.1.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/redux-immutable-state-invariant": "^2.1.4",
    "prettier": "^3.2.4",
    "react-native-web": "^0.19.12",
    "typescript": "^5.1.3"
  },
  "private": true
}
